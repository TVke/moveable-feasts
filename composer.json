{
    "name": "ppelgrims/moveable-feasts",
    "description": "Carbon Mixing",
    "homepage": "https://github.com/ppelgrims/MoveableFeasts",
    "keywords": ["carbon", "mixing", "moveable", "movable", "feasts", "easter"],
    "version": "1.0.1",
    "license": "MIT",
    "authors": [
        { "name": "Pieter Pelgrims" }
    ],
    "require": {
        "php": "^5.4 || ^7.0",
        "nesbot/carbon": "~1.0|~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35 || ^5.7",
        "squizlabs/php_codesniffer": "^2.0",
        "ext-intl": "*"
    },
    "autoload": {
        "psr-4": {
            "MoveableFeasts\\": "src/MoveableFeasts/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MoveableFeasts\\Test\\": "tests/MoveableFeasts/"
        }
    },
    "scripts": {
        "phpcs": "vendor/bin/phpcs --standard=PSR2 -n",
        "phpcbf": "vendor/bin/phpcbf --standard=PSR2 -n",
        "phpunit": "phpunit --colors=always --verbose"
    }
}
